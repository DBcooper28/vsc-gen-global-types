{
  "name": "vsc-gen-global-types",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.47.3"
  },
  "icon": "images/icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "publisher": "zyx28",
  "repository": {
    "url": "https://github.com/DBcooper28/vsc-gen-global-types"
  },
  "license": "MIT",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.readFiles",
        "title": "生成类型声明文件global"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.readFiles",
          "group": "navigation"
        }
      ]
    }
  },
	"scripts": {
		"build:asar": "asar pack ./node_modules ./dist/node_modules.asar"
  },
  "devDependencies": {
    "asar": "^3.2.0"
  }
}
